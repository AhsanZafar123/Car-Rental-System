{
	"name": "car-rental-system",
	"version": "1.0.0",
	"description": "car rental system using mern stack tech",
	"main": "server.js",
	"type": "module",
	"scripts": {
		"start": "node backend/server.js",
		"server": "nodemon backend/server.js",
		"client": "npm run dev --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"data:import": "node backend/seeder",
		"data:destroy": "node backend/seeder -d",
		"build": "npm install && npm install --prefix client && npm run build --prefix client",
		"generate-toc": "markdown-toc -i readme.md"
	},
	"keywords": [
		"car-rental",
		"rental",
		"car",
		"motor-car",
		"motor",
		"rent",
		"drive"
	],
	"author": "Naimat Ullah",
	"license": "ISC",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"redis": "^4.7.0",
		"slugify": "^1.6.6",
		"validator": "^13.12.0"
	},
	"devDependencies": {
		"concurrently": "^8.2.2",
		"nodemon": "^3.1.4"
	}
}
